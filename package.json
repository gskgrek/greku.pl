    },
    "bugs": {
        "url": "https://github.com/gatsbyjs/gatsby/issues"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.js": [
            "prettier --config .prettierrc --write",
            "eslint --fix",
            "stylelint --fix",
            "git add"
        ]
    },
    "packageManager": "yarn@3.2.3"
